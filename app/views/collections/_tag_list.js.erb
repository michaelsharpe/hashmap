<% @collection.collectionTags.each do |tag| %>
  <li data-tag="<%= tag.name %>" data-show="enabled" class="sortable-active"><a><span class='handle'><i class='fa fa-sort'></i></span><i class="tag-eye fa fa-eye"></i><span><%= tag.name %></span></a></li>
<% end %>
<li class="tag-form" style="display: none">
  <span>
    <div>
      <%= render partial: "maps/tag_form", locals: {collection: @collection, user: @collection.user} %>
    </div>
  </span>
</li>
<li class="new-tag">
  <a><span>Add a Tag!</span></a>
</li>
  