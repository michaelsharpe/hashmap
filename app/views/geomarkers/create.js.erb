<% if @geomarker.errors.any? %>
  $('#geomarker-form-errors').html('<%= escape_javascript(render "geomarker_form_errors") %>' );
<% else %>
  getGeomarkerShow(<%= @geomarker.id %>);
  $("#new-marker-button").trigger("click");
  getNewMarkerByUser();
  removeIFrame();
  removeGeomarkerForm();
<% end %>